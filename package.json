{
  "name": "portfolio-corentin-hervaud",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "test:unit": "vitest run",
    "test:unit:dev": "vitest --ui",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "start-server-and-test 'NITRO_PORT=4173 nuxt preview' http://localhost:4173 'cypress run --e2e'",
    "test:e2e:record": "start-server-and-test 'NITRO_PORT=4173 nuxt preview' http://localhost:4173 'cypress run --e2e --record'",
    "test:e2e:dev": "start-server-and-test dev http://localhost:4173 'cypress open --e2e' --browser chrome",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "nuxt": "^3.11.2",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.39",
    "@nuxt/test-utils": "^3.13.1",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "cypress": "^13.10.0",
    "happy-dom": "^14.11.0",
    "start-server-and-test": "^2.0.3",
    "typescript": "^5.4.5",
    "vite-plugin-istanbul": "^6.0.2",
    "vitest": "^1.6.0"
  }
}
